          "editorMode": "code",
          "format": 1,
          "location": "",
          "project": "mlab-collaboration",
          "rawQuery": true,
          "rawSql": "# 2025-05-30 Estimate egress value\n\n# was 2025-01-29 Measure per site load\n\n# A fresher version is at https://grafana.mlab-sandbox.measurementlab.net/d/eend62wpzz94wa/estimate-egress-value?orgId=1&editPanel=1\n# see also https://grafana.mlab-sandbox.measurementlab.net/d/ce0h5fgtyy48wc/bigquery3a-ndt7-client-integration-load-metrics?orgId=1\nWITH\n\nndt7_managed AS ( -- Production\n    SELECT\n      REGEXP_EXTRACT(server.site, r'^[a-z]{3}') AS metro,\n      date,\n      CONCAT (server.site, '-', server.machine) AS server,\n      _internal202402.FinalSnapshot.TCPInfo.BytesSent / 1000.0 AS sentkB,\n    FROM `measurement-lab.ndt_intermediate.extended_ndt7_downloads`\n    WHERE date BETWEEN '2025-05-19' AND '2025-05-25'\n    # WHERE date BETWEEN DATE_SUB(\"$endDate\", INTERVAL ${duration:raw}-1 DAY) and DATE(\"$endDate\")\n  ),\n  \n  ndt7_autonode AS (  -- autojoin, includes BYOS\n    SELECT\n      REGEXP_EXTRACT(server.site, r'^[a-z]{3}') AS metro,\n      date,\n      CONCAT (server.site, '-', server.machine) AS server,\n      (   IFNULL((SELECT MAX(TCPinfo.BytesSent) FROM UNNEST(raw.Download.ServerMeasurements)), 0.0) +\n          IFNULL((SELECT MAX(TCPinfo.BytesSent) FROM UNNEST(raw.Upload.ServerMeasurements)), 0.0)\n      ) / 1000.0 AS sentkB,\n    FROM `mlab-autojoin.autoload_v2_ndt.ndt7_union`\n    WHERE date BETWEEN '2025-05-19' AND '2025-05-25'\n    # WHERE date BETWEEN DATE_SUB(\"$endDate\", INTERVAL ${duration:raw}-1 DAY) and DATE(\"$endDate\")\n  ),\n\nndt7 AS (\n  SELECT * FROM ndt7_managed UNION ALL\n  SELECT * FROM ndt7_autonode\n),\n\nserverMetadata AS (\n  SELECT *,\n    0.10 AS estPrice\n  FROM (\n    SELECT\n    Server, site, \n    FORMAT('%t %t',City, CountryCode) AS location,\n    CASE\n      WHEN zone IS NOT NULL THEN 'GCE' # FORMAT('GCE-%t',REGEXP_EXTRACT(zone, '[a-z]*'))  -- Consider adding attributes\n      WHEN managed LIKE '%switch%' THEN 'LegacyFleet'\n      WHEN managed LIKE '%machine%' THEN 'SingletonFleet'\n      WHEN deployment LIKE 'byos' AND type LIKE 'physical' THEN 'BYOS-physical'\n      WHEN deployment LIKE 'byos' AND type LIKE 'virtual' THEN 'BYOS-virtual'\n      ELSE 'UNKNOWN'\n    END as MLabSKU,\n    tag,\n    managed, deployment, org, type, machineType, zone, loadBalanced,\n    FROM `mlab-collaboration.mm_preproduction.cached_metadata`\n  )\n),\n\n-- Everything we know about each server\nserverData AS ( -- Trafic is normailized to per day\n  SELECT *,\n    SUM(tests) OVER (PARTITION BY metro) AS metroTests,\n    SUM(egressMB) OVER (PARTITION BY metro) AS metroEgressMB,\n  FROM (\n    SELECT\n      metro, server,      \n      COUNT(*) * 1.0 / COUNT (DISTINCT date) AS tests,\n      SUM(sentkB) /1000.0 / COUNT (DISTINCT date) AS egressMB,\n    FROM ndt7 \n    GROUP BY metro, server\n  )  JOIN serverMetadata USING (server)\n),\n\nmetroCosts AS (\n  SELECT\n    metro, location, tag, server, site, MLabSKU,\n    REGEXP_extract(tag,'[A-Z]*') AS cont,\n    ROUND(tests) AS dailyTests,\n    ROUND(100.0*tests / metroTests ) AS pctTests,\n    ROUND(100.0*egressMB / metroEgressMB ) AS pctEgress,\n    ROUND( egressMB * 8 / (24*60*60)  ) AS egressMbps,\n    ROUND( egressMB * 30.0 / 1000000.0 ) AS egrMonthlyTiB,\n    estPrice,\n    ROUND( egressMB * estPrice / 1000.0 ) AS estDailyCostUSD,\n    # ROUND( egressMB * estPrice / 1000000.0 * 30 ) AS estMonthlyCostKUSD,\n    ROUND( egressMB * estPrice / 1000000.0 * 365 ) AS estAnualCostKUSD,\n  FROM serverData\n  # WHERE zone IS NOT NULL\n  # ORDER BY estDailyCostUSD DESC\n),\n\nmetroInventory AS (\n  SELECT\n    metro,\n    MLabSKU,\n    ANY_VALUE(location) AS Location, \n    COUNT (DISTINCT server) AS servers,\n    COUNT (DISTINCT site) AS sites,\n    SUM(dailyTests) AS dailyTests,\n    SUM(pctTests) AS  pctTests,\n    SUM(pctEgress) AS  pctEgress,\n    SUM(egressMbps) AS  egressMbps,\n    SUM(egrMonthlyTiB) AS egrMonthlyTiB,\n    ANY_VALUE(estPrice) AS estPrice,\n    SUM(estDailyCostUSD) AS estDailyCostUSD,\n    SUM(estAnualCostKUSD) AS estAnualCostKUSD,\n    MIN(tag) AS tag,\n  FROM metroCosts\n  GROUP BY metro, MLabSKU\n),\n\ns
