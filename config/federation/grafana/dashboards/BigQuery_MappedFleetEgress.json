          "editorMode": "code",
          "format": 1,
          "location": "",
          "project": "mlab-collaboration",
          "rawQuery": true,
          "rawSql": "\nSELECT * EXCEPT (tag)\nFROM (\n    SELECT latitude, longitude,\n        CASE level \n            WHEN 'Subtotal by Metro' THEN FORMAT(' %t total egressMbps:%t Servers:%t Sites:%t', metro, egressMbps, Servers, Sites ) \n            WHEN 'Subtotal by Metro-SKU' THEN FORMAT('%t egressMbps:%t Servers:%t Sites%t', MLabSKU, egressMbps, Servers, Sites )\n            ELSE CAST(NULL AS STRING)\n        END AS S,  -- Reduce clutter\n        FORMAT('%t estDailyCostUSD:%t at estPrice:%t per GB', MLabSKU, estDailyCostUSD, estPrice) AS C,  -- Reduce clutter\n        MLabSKU, metro, tag\n    FROM `mm_preproduction.global_fleet_inventory`(DATE_SUB(\"$endDate\", INTERVAL ${duration:raw}-1 DAY), DATE(\"$endDate\"))\n    WHERE tag NOT LIKE '%!'\n)\nWHERE S is not NULL\nORDER BY tag, MLabSKU DESC\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "MLab Fleet Average Load for ${duration:raw} days ending $endDate",
      "transformations": [
