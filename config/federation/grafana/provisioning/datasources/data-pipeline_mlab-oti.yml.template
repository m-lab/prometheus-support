# config file version
apiVersion: 1

datasources:
- name: Data Pipeline (mlab-oti)
  type: prometheus
  access: proxy
  orgId: 1
  url: https://prometheus-data-pipeline.mlab-oti.measurementlab.net
  isDefault: {{IS_DEFAULT}}
  version: 1
  editable: false

  jsonData:
    timeout: 60
    timeInterval: 60s
    httpMethod: POST
    keepCookies:
    - _oauth2_proxy
